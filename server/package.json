{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "start": "pm2 start ./build/src/app.js -i 1 --attach --watch",
    "stop": "pm2 stop all",
    "delete": "pm2 delete all",
    "dev": "nodemon -r tsconfig-paths/register  --exec 'ts-node' --files src/app.ts",
    "dev-debug": "nodemon -r tsconfig-paths/register  --exec 'ts-node' --files src/app.ts",
    "dev:seed": "nodemon -r tsconfig-paths/register  --exec 'ts-node' --files src/app.ts -- --seed-data",
    "build": "rm -rf build && tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "prettier:check": "prettier --check 'src/**/*.{ts,json}'",
    "prettier:fix": "prettier --write 'src/**/*.{ts,json}'",
    "test": "jest --coverage=true -w=1 --forceExit --detectOpenHandles --watchAll=false"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
