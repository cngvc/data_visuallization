    "@jest/types": "^30.0.5",
    "adm-zip": "^0.5.16",
    "agenda": "^5.0.0",
    "argon2": "^0.44.0",
    "axios": "^1.8.4",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "crypto-random-string": "^5.0.0",
    "csv-parse": "^6.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.3.0",
    "jest": "^30.0.5",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^8.18.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.6",
    "reflect-metadata": "^0.2.2",
    "slug": "^11.0.0",
